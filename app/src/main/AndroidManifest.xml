<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.restaurant.akmal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Chef Akmal"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.restaurant.akmal.DashboardActivity" />
        <activity android:name="com.restaurant.akmal.ProfileActivity" />
        <activity android:name="com.restaurant.akmal.ChangePasswordActivity" />
        <activity android:name="com.restaurant.akmal.auth.RegisterActivity" />
        <activity android:name="com.restaurant.akmal.auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>